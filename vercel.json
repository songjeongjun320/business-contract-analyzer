{
  "functions": {
    "pages/api/**/*.ts": {
      "runtime": "@vercel/node@2.0.0",
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "routes": [
    { "src": "/api/upload", "dest": "pages/api/upload/route.ts" },
    {
      "src": "/api/get-final-result",
      "dest": "pages/api/get-final-result/route.ts"
    }
  ]
}
